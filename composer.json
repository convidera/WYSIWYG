{
    "name": "convidera/wysiwyg",
    "description": "A basic and simple WYSIWYG editor for sstext of homepages build with the php templating engine blade.",
    "keywords": ["WYSIWYG", "composer", "package", "blade"],
    "type": "project",
    "license": "GPL-3.0-or-later",
    "version": "0.0.1.28",
    "authors": [
        {
            "name": "Florian Heinrich",
            "email": "heinrich@convidera.com"
        }
    ],
    "support": {
		"issues": "https://github.com/heinrichconvidera/wysiwyg/issues",
		"source": "https://github.com/heinrichconvidera/wysiwyg"
	},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "ramsey/uuid": "^3.8.0",
        "wehaa/liveupdate": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Convidera\\WYSIWYG\\": "src/app/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Convidera\\WYSIWYG\\Providers\\WYSIWYGServiceProvider"
        ]
      }
    }
}
