{
    "name": "convidera/wysiwyg",
    "description": "A basic and simple WYSIWYG editor for sstext of homepages build with the php templating engine blade.",
    "keywords": ["WYSIWYG", "composer", "package", "blade"],
    "type": "project",
    "license": "GPL-3.0-or-later",
    "version": "0.0.2.0",
    "authors": [
        {
            "name": "Florian Heinrich",
            "email": "heinrich@convidera.com"
        }
    ],
    "support": {
		"issues": "https://github.com/convidera/WYSIWYG/issues",
		"source": "https://github.com/convidera/WYSIWYG"
	},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "wehaa/liveupdate": "^0.1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "laravel/framework": "5.8.*",
        "ramsey/uuid": "^3.8.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Convidera\\WYSIWYG\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts-descriptions": {
        "test": "Run all tests!"
    },
    "scripts": {
        "test": "npm test"
    },
    "extra": {
      "laravel": {
        "providers": [
          "Convidera\\WYSIWYG\\Providers\\WYSIWYGServiceProvider"
        ]
      }
    }
}
